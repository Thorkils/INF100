class Person2 
{
  
  String navn; 
  
  
  void settNavn(String personNavn) 
  {
    navn = personNavn;
  }
  
  String hentNavn() 
  {
    return navn;
  }
  
  
  
  public String toString() 
  {
    return ("Person " + navn);
  }
  
}

class Interesse2 
{
  
  String interesse;
  
  void settInteresse(String personInteresse) 
  {
    interesse = personInteresse;
  }
  
  String hentInteresse() 
  {
    return interesse;
  }
  
  
  
  public String toString() 
  {
    return ("- Interesse:  " + interesse);
  } 
}
